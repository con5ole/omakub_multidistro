{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#16CB48>\u250f[</>{{ .UserName }}<#16CB48>]</>",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#16CB48>[</>{{ .HEAD }}<#16CB48>]</>",
          "type": "git"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#16CB48>[</>\uf0e7<#16CB48>]</>",
          "type": "root"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#16CB48>[x</>{{ reason .Code }}<#16CB48>]</>",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<#16CB48>\u2516[</>{{ .Path }}<#16CB48>]></>",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
